\pard\plain \s16\ql \li0\ri0\sl240\slmult0\widctlpar\tx461\faauto\adjustright\rin0\lin0\itap0 \f1\fs20\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 {\cs15\fs16\up6\insrsid12456095 A}{\insrsid12456095  HID_MATRIX}}K{\footnote \pard\plain 
